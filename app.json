{
  "expo": {
    "name": "twitsnap",
    "slug": "tutorial",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/fiuba_sticker.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/fiuba_sticker.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.franciscoduc4.tutorial"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "a7594217-538a-4552-9896-2f0fee48fd88"
      }
    },
    "owner": "franciscoduc4"
  }
}
